<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/activity_my_data_entities_certification_business_license"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <include layout="@layout/top_main"/>


    <ImageView
        android:id="@+id/pic_activity_my_data_entities_certification_business_license"
        android:layout_weight="1"
        android:src="@mipmap/ic_launcher"
        android:scaleType="centerInside"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:id="@+id/submit_activity_my_data_entities_certification_business_license"
            android:text="提交审核"
            android:textSize="14sp"
            android:gravity="center"
            android:textColor="#fff"
            android:background="@color/mainColor"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
</LinearLayout>
